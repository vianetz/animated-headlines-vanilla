{
  "name": "animated-headlines-vanilla",
  "version": "2.0.0",
  "description": "Simple animated headlines without Jquery!",
  "main": "dist/js/animated-headline.js",
  "scripts": {
    "dev": "NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "hot": "NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "author": "Geoff Selby",
  "contributors": [
    "Christoph Massmann <cm@vianetz.com> (https://vianetz.com/)"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vianetz/animated-headlines-vanilla.git"
  },
  "keywords": [
    "animated headlines",
    "typewriter"
  ],
  "devDependencies": {
    "laravel-mix": "^0.8.1"
  }
}
