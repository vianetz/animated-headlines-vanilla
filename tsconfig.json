{
  "compilerOptions": {
    "target": "es2022",
    "module": "commonjs",
    "declaration": false,
    "outDir": "dist/js/",
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": false,
    "typeRoots": [
      "node_modules/@types"
    ],
    "downlevelIteration": true
  },
  "include": [
    "src/**/*"
  ]
}